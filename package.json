{
  "name": "@storeon/vue",
  "version": "1.0.0",
  "description": "A tiny (160 bytes) connector for Storeon and Vue",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "yarn lint && jest --coverage && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storeon/vue.git"
  },
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storeon/vue/issues"
  },
  "homepage": "https://github.com/storeon/vue#readme",
  "keywords": [
    "vue",
    "storeon",
    "state"
  ],
  "files": [
    "index.d.ts",
    "helpers/",
    "class/"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@vue/composition-api": "^0.5.0",
    "storeon": "^2.0.0",
    "vue": "^2.6.0",
    "vue-class-component": "^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "vue-class-component": {
      "optional": true
    },
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@logux/eslint-config": "^36.1.2",
    "@size-limit/dual-publish": "^4.4.5",
    "@size-limit/preset-small-lib": "^4.4.5",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/composition-api": "^0.5.0",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "dual-publish": "^0.10.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^18.0.1",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "parcel-bundler": "^1.12.4",
    "size-limit": "^4.4.5",
    "storeon": "^2.0.0",
    "vue": "^2.6.0",
    "vue-class-component": "^6.0.0 || ^7.0.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "name": "core",
      "import": {
        "index.js": "{ StoreonVue }"
      },
      "limit": "160 B"
    },
    {
      "name": "core + helpers",
      "import": {
        "index.js": "{ StoreonVue }",
        "helpers/index.js": "{ mapState, mapDispatch }"
      },
      "limit": "342 B"
    },
    {
      "name": "core + class",
      "import": {
        "index.js": "{ StoreonVue }",
        "class/index.js": "{ State, Dispatch }"
      },
      "limit": "447 B"
    },
    {
      "name": "composition",
      "import": {
        "composition/index.js": "{ provideStoreon, useStoreon }"
      },
      "limit": "175 B"
    }
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "example"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "test/utils.js"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  }
}
