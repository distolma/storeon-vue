{
  "name": "@storeon/vue",
  "version": "0.5.0",
  "description": "A tiny (150 bytes) connector for Storeon and Vue",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "yarn lint && jest --coverage && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storeon/vue.git"
  },
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storeon/vue/issues"
  },
  "homepage": "https://github.com/storeon/vue#readme",
  "peerDependencies": {
    "storeon": "^2.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@logux/eslint-config": "^36.1.1",
    "@size-limit/dual-publish": "^4.4.1",
    "@size-limit/preset-small-lib": "^4.4.1",
    "@vue/component-compiler-utils": "^3.1.1",
    "@vue/test-utils": "^1.0.0-beta.32",
    "dual-publish": "^0.8.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^17.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "parcel-bundler": "^1.12.4",
    "size-limit": "^4.4.1",
    "storeon": "^2.0.1",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "name": "core",
      "import": {
        "index.js": "{ StoreonVue }"
      },
      "limit": "136 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "node_modules",
    "example"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "test/utils.js"
    ]
  },
  "keywords": [
    "vue",
    "storeon",
    "state"
  ]
}
