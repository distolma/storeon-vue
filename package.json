{
  "name": "@storeon/vue",
  "version": "3.0.0-beta.1",
  "description": "A tiny (160 bytes) connector for Storeon and Vue",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "yarn lint && jest --coverage && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storeon/vue.git"
  },
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storeon/vue/issues"
  },
  "homepage": "https://github.com/storeon/vue#readme",
  "keywords": [
    "vue",
    "storeon",
    "state"
  ],
  "files": [
    "index.d.ts",
    "helpers/",
    "class/",
    "composition/"
  ],
  "sideEffects": false,
  "dependencies": {
    "vue": "^3.0.0-beta.15"
  },
  "peerDependencies": {
    "storeon": "^3.0.0",
    "vue-class-component": "^8.0.0-alpha.6"
  },
  "peerDependenciesMeta": {
    "vue-class-component": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@logux/eslint-config": "^39.0.3",
    "@size-limit/dual-publish": "^4.5.1",
    "@size-limit/preset-small-lib": "^4.5.1",
    "@vue/test-utils": "^2.0.0-alpha.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "dual-publish": "^0.10.5",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-prettierx": "^0.12.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^20.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "parcel": "^2.0.0-beta.1",
    "size-limit": "^4.5.1",
    "storeon": "^3.0.1",
    "vue": "^3.0.0-beta.15",
    "vue-class-component": "^8.0.0-alpha.6"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "name": "core",
      "import": {
        "index.js": "{ StoreonVue }"
      },
      "limit": "160 B"
    },
    {
      "name": "core + helpers",
      "import": {
        "index.js": "{ StoreonVue }",
        "helpers/index.js": "{ mapState, mapDispatch }"
      },
      "limit": "342 B"
    },
    {
      "name": "core + class",
      "import": {
        "index.js": "{ StoreonVue }",
        "class/index.js": "{ State, Dispatch }"
      },
      "limit": "447 B"
    },
    {
      "name": "composition",
      "import": {
        "composition/index.js": "{ provideStoreon, useStoreon }"
      },
      "limit": "175 B"
    }
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "example"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "test/utils.js"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  }
}
