{
  "name": "storeon-vue",
  "version": "0.1.0",
  "description": "A tiny (135 bytes) connector for Storeon and Vue",
  "main": "index.js",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "yarn lint && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distolma/storeon-vue.git"
  },
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distolma/storeon-vue/issues"
  },
  "homepage": "https://github.com/distolma/storeon-vue#readme",
  "dependencies": {
    "storeon": "^1.0.0",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@logux/eslint-config": "^36.0.0",
    "@size-limit/preset-small-lib": "^4.1.1",
    "@vue/component-compiler-utils": "^3.1.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^16.1.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "parcel-bundler": "^1.12.4",
    "size-limit": "^4.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "storeon": "^1.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "path": "index.js",
      "limit": "135 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "eslintIgnore": [
    "node_modules",
    "example"
  ],
  "keywords": [
    "vue",
    "storeon",
    "state"
  ]
}
